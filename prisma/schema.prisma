generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Booking {
  id          String   @id @default(uuid())
  startTime   DateTime
  endTime     DateTime
  user        String
  deleteToken String   @default(uuid())
  deviceToken String?  // Add optional deviceToken for rate limiting
  createdAt   DateTime @default(now())
}
